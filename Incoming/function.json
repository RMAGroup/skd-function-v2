    {
      "name": "inBlob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "incoming/{name}"
    },
    {
      "name": "bomBlob",
      "type": "blob",
      "direction": "out",
      "path": "bom/{name}"
    },
    {
      "name": "shipBlob",
      "type": "blob",
      "direction": "out",
